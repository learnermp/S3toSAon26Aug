{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-26aug"
		},
		"LS_Gen2_Output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_Gen2_Output'"
		},
		"LS_Gen2_Output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sa26aug.dfs.core.windows.net/"
		},
		"LS_KV_secrets_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-26aug.vault.azure.net/"
		},
		"LS_S3_input_properties_typeProperties_accessKeyId": {
			"type": "object",
			"defaultValue": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "LS_KV_secrets",
					"type": "LinkedServiceReference"
				},
				"secretName": "aws-mitee-accesskey"
			}
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-26aug_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_Gen2_Output_accountKey": {
						"value": "[parameters('LS_Gen2_Output_accountKey')]"
					},
					"LS_Gen2_Output_properties_typeProperties_url": {
						"value": "[parameters('LS_Gen2_Output_properties_typeProperties_url')]"
					},
					"LS_KV_secrets_properties_typeProperties_baseUrl": {
						"value": "[parameters('LS_KV_secrets_properties_typeProperties_baseUrl')]"
					},
					"LS_S3_input_properties_typeProperties_accessKeyId": {
						"value": "[parameters('LS_S3_input_properties_typeProperties_accessKeyId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}